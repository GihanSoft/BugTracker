{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.3.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "css/*",
        "js/*"
      ]
    },
    {
      "library": "bootstrap-icons@1.11.3",
      "destination": "wwwroot/lib/bootstrap-icons/",
      "files": [
        "bootstrap-icons.svg"
      ]
    },
    {
      "library": "mdui@2.1.2",
      "destination": "wwwroot/lib/mdui/",
      "files": [
        "mdui.min.css",
        "mdui.global.min.js",
        "locales/fa-ir.min.js"
      ]
    }
  ]
}